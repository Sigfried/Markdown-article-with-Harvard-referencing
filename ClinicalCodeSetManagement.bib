@article{shipmanFormalityConsideredHarmful1999,
  title = {Formality {{Considered Harmful}}: {{Experiences}}, {{Emerging Themes}}, and {{Directions}} on the {{Use}} of {{Formal Representations}} in {{Interactive Systems}}},
