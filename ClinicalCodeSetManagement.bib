@article{thomerTransformingTaxonomicInterfaces2018,
  title = {Transforming {{Taxonomic Interfaces}}: "{{Arm}}?S {{Length}}" {{Cooperative Work}} and the {{Maintenance}} of a {{Long}}-Lived {{Classification System}}},
